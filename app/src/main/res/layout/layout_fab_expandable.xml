<?xml version="1.0" encoding="utf-8"?>
<com.getbase.floatingactionbutton.FloatingActionsMenu xmlns:android="http://schemas.android.com/apk/res/android" xmlns:fab="http://schemas.android.com/apk/res-auto"
    style="@style/FABEx_menu_button"
    android:id="@+id/fab_menu"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/transparent"
    android:clickable="false"
    android:focusable="false"
    fab:fab_expandDirection="up"
    fab:fab_labelStyle="@style/FABEx_label">

    <com.getbase.floatingactionbutton.FloatingActionButton
        style="@style/FABEx_menu_subbutton"
        android:id="@+id/fab_new_poi"
        fab:fab_icon="@drawable/icon_map_menu_crosshair_30dp"/>

    <com.getbase.floatingactionbutton.FloatingActionButton
        style="@style/FABEx_menu_subbutton"
        android:id="@+id/fab_push_registration"
        fab:fab_icon="@drawable/icon_map_menu_push_30dp"/>

    <com.getbase.floatingactionbutton.FloatingActionButton
        style="@style/FABEx_menu_subbutton"
        android:id="@+id/fab_map_type"
        fab:fab_icon="@drawable/icon_map_menu_map_30dp"/>

    <com.getbase.floatingactionbutton.FloatingActionButton
        style="@style/FABEx_menu_subbutton"
        android:id="@+id/fab_map_filter"
        fab:fab_icon="@drawable/icon_filter_30dp"/>

</com.getbase.floatingactionbutton.FloatingActionsMenu>